base="zoneinfo-all luci luci-compat luci-lib-ipkg -dnsmasq dnsmasq-full luci-i18n-base-zh-cn luci-i18n-firewall-zh-cn openssl-util"
if [ "$OPENWRT_VERSION" == "24.10" ] || [ "$IS_SNAPSHOT_BUILD" == "1" ]; then
    base="$base luci-i18n-package-manager-zh-cn"
else
    base="$base luci-i18n-opkg-zh-cn"
fi
echo $base